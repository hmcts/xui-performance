{
  "type": "event",
  "event": "IcpUpdateScreen",
  "data": {
    "body": {
      "pdfPosition": {
        "pageNumber": #{randomNumberBetweenXandY},
        "scale": 100,
        "top": #{message_top},
        "left": #{message_left},
        "rotation": 0,
        "pdfOpenParams": "#page=#{randomNumberBetweenXandY}&zoom=100,#{message_left},#{message_top}"
      }
    },
    "caseId": "#{caseId}",
    "documentId": "#{docId}"
  }
}